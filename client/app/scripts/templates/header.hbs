<div class="row">
  <div class="columns col12">
    <ul class="no-bullet">
      <li class="title animate">What is Our Project Named?</li>
      <ul class="menu no-bullet">
        {{#if username}}
          <li class="animate"><a class="login" href="#">{{ username }}</a></li>
          <li class="animate"><a class="logout"  href="/api/auth/logout">logout</a></li>
        {{else}}
          <li class="animate"><a class="login" href="/api/auth/login">login</a></li>
        {{/if}}
      </ul>
    </ul>
  </div>
</div>

